program factorial;
var x, res : int;

int fact(n: int) {
    var ans : int;
    {
        if (n < 2) {
            return 1;
        } else {
            ans = n * fact(n - 1);
            return ans;
        }
    }
};

main() {
    x = 5;
    res = fact(x);
    print("Factorial of", x, "is", res);
    
    x = 6;
    print("Factorial of", x, "is", fact(x));
}
end

