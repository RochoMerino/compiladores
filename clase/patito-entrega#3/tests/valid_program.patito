// Programa válido con todas las características semánticas correctas
program calculator;

var globalX, globalY : int;
    globalResult : float;

void add(a : int, b : int) [
    var temp : int;
{
    temp = a + b;
    print("Sum is:", temp);
}
];

void multiply(x : float, y : float) [
    var result : float;
{
    result = x * y;
    print("Product is:", result);
}
];

main() {
    globalX = 10;
    globalY = 5;
    globalResult = globalX + globalY * 2.5;

    print("Global result:", globalResult);

    if (globalX > globalY) {
        print("X is greater");
    } else {
        print("Y is greater or equal");
    };

    add(globalX, globalY);
    multiply(3.14, 2.0);
}
end
