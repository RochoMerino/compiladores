// Test: Complex arithmetic expressions with precedence
program complex_expressions;

var a, b, c, d, e : int;
    result : float;

main() {
    a = 5;
    b = 3;
    c = 2;
    d = 4;

    e = a + b * c;
    e = a * b + c;
    e = a + b - c * d;
    e = (a + b) * c;

    result = a + b * c / d - e;
}
end
